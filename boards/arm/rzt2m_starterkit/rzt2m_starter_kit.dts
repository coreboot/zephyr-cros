/dts-v1/;
#include <arm/renesas/rzt2m.dtsi>

/ {
	model = "RZT/2M Starter Kit";
	compatible = "renesas,rzt2m_starter_kit";

	chosen {
		zephyr,sram = &cpu0_atcm;
		zephyr,console = &uart0;
		zephyr,shell-uart = &uart0;
	};
};

&uart0 {
	status = "okay";
};

&uart3 {
	status = "okay";
};
