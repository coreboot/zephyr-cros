/dts-v1/;

#include <espressif/esp32c3.dtsi>

/ {
	model = "esp32c3_devkitm";
	compatible = "espressif,esp32c3";

	chosen {
		zephyr,sram = &sram0;
		zephyr,console = &uart0;
		zephyr,shell-uart = &uart0;
	};
};

&uart0 {
	status = "okay";
	current-speed = <115200>;
};

&trng0 {
	status = "okay";
};
