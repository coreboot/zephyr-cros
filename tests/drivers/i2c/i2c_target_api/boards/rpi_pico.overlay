&i2c0 {
	eeprom0: eeprom@54 {
		compatible = "atmel,at24";
		reg = <0x54>;
		size = <1024>;
		pagesize = <16>;
		address-width = <8>;
		timeout = <5>;
	};
};

&i2c1 {
	eeprom1: eeprom@56 {
		compatible = "atmel,at24";
		reg = <0x56>;
		size = <1024>;
		pagesize = <16>;
		address-width = <8>;
		timeout = <5>;
	};
};
